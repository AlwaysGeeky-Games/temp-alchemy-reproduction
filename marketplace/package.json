{
  "name": "voxies-marketplace",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "prod": "env-cmd -f .env.production next dev -p 3001",
    "staging": "env-cmd -f .env.staging next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stories": "npx ladle serve --stories=components/**/*.stories.{ts,tsx}",
    "prettier-format": "npx pretty-quick --staged",
    "prepare": "husky",
    "test": "jest --config jest.config.ts",
    "test-coverage": "jest --config jest.config.ts --collectCoverage",
    "playwright": "npx playwright test",
    "playwright:codegen": "npx playwright codegen",
    "playwright:ui": "npx playwright test endToEndTests --ui",
    "playwright:pages": "npx playwright test endToEndTests/tests/pages",
    "playwright:others": "npx playwright test endToEndTests/tests/others",
    "playwright:desktop-chrome": "BROWSER=chromium npx playwright test",
    "playwright:desktop-firefox": "BROWSER=firefox npx playwright test",
    "playwright:mobile-chrome": "BROWSER='mobile-chrome' npx playwright test",
    "playwright:mobile-safari": "BROWSER='mobile-safari' npx playwright test",
    "e2e": "concurrently -n endToEndTests \"npm run dev\" \"wait-on http://localhost:3001 && env-cmd -f .env.test npm run playwright:ui\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "abi-ts": "typechain --target=ethers-v6 \"**/*.abi.json\" --out-dir contracts-utils/types"
  },
  "dependencies": {
    "@aa-sdk/core": "^4.50.1",
    "@account-kit/core": "^4.50.1",
    "@account-kit/infra": "^4.50.1",
    "@account-kit/react": "^4.50.1",
    "@account-kit/signer": "^4.50.1",
    "@account-kit/smart-contracts": "^4.50.1",
    "@tanstack/react-query": "^5.51.24",
    "@tanstack/react-query-devtools": "^5.59.15",
    "animate.css": "^4.1.1",
    "daisyui": "^2.52.0",
    "ethers": "^6.13.0",
    "next": "^14.2.30",
    "next-themes": "^0.3.0",
    "ramda": "^0.29.1",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.3",
    "three": "^0.165.0",
    "viem": "^2.21.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.1.4",
    "@next/polyfill-module": "^14.2.13",
    "@playwright/test": "^1.45.1",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/nextjs": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@swc/core": "^1.9.2",
    "@swc/jest": "^0.2.36",
    "@synthetixio/synpress": "^4.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/ramda": "^0.29.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eth-testing": "^1.14.0",
    "headless-web3-provider": "^0.2.3",
    "husky": "^9.0.11",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.3",
    "wait-on": "^7.2.0"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
