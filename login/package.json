    "@account-kit/infra": "^4.50.1",
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-query-devtools": "^5.59.15",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "viem": "^2.21.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@swc/core": "^1.7.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
