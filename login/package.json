{
  "name": "voxies-login",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3002",
    "prod": "env-cmd -f .env.production next dev -p 3002",
    "staging": "env-cmd -f .env.staging next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config jest.config.ts",
    "test-coverage": "npm run test -- --coverage",
    "test-dev": "env-cmd -f .env.test next dev -p 3002",
    "playwright": "npx playwright test",
    "playwright:codegen": "npx playwright codegen",
    "playwright:ui": "npx playwright test endToEndTests --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@aa-sdk/core": "^4.50.1",
    "@account-kit/core": "^4.50.1",
    "@account-kit/react": "^4.50.1",
